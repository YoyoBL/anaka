generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Feeding {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  breastSide BrestSide 
  createdAt DateTime @default(now())
}

enum BrestSide {
  right
  left
}

model Diapers {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  pee Boolean @default(true)
  poop Boolean @default(false)
  createdAt DateTime @default(now())
}

model Sleep {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
 status Status @default(START)
  createdAt DateTime @default(now())
}

enum Status {
  START
  END
}


